<h1>signup page<h1>

<%= form_with model: @user, url:'/login' do |form|%>
<div>
	<%= form.label :name %><br>
	<%= form.text_field :name %>
</div>
<div>
	<%= form.label :email %><br>
	<%= form.text_field :email %>
</div>
<div>
	<%= form.label :password %><br>
	<%= form.text_field :password %>
</div>
<div>
	<%= form.label :status %><br>
	<%= form.text_field :status %>
</div>
<div>
	<%= form.submit %><br>
	
</div>
<%end%>